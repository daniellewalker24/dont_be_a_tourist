<% @include_tinymce_script = true %>


<%= f.error_messages %>

<p>
  <%= f.label :name %><br />
  <%= f.text_field :name %>
</p>

<p>
  <%= f.label :summary %><br />
  <%= f.text_area :summary %>
</p>

<p>
  <%= f.label :content %><br />
  <%= f.tinymce_text_area :content %>
</p>

<p>
  <%= f.submit 'Submit' %>
</p>
