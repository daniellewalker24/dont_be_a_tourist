<%% @page_title = "<%= print_plural_name %>" %><h1><%= @page_title %></h1><%%= render :partial => 'navigation' %><div id="news"><%% if @list_name -%>  <h2><%%= @list_name%></h2><%% else -%>  <h2><%%= month_name(session[:<%= feature_name %>][:month].to_i) + " - " + params[:year].to_s %></h2><%% end -%>  <%% if @<%= plural_name %>.length != 0 -%>  <%% for <%= singular_name %> in @<%=  plural_name %> -%>    <hr />    <%%= render( :partial => 'summary',                      :locals => {:<%= singular_name %> => <%= singular_name %>}) %>  <%% end -%>    <hr />  <%% else -%>  <p><strong>There are currently no <%= print_plural_name %> to be displayed.</strong></p>  <%% end -%>  <%% if @<%= singular_name %>_pages.count != 0 %>  <div class="pagecount">    <%% if @<%= singular_name %>_pages.previous_page -%>      <%%= link_to '&lt;&lt; Back', :action => 'list', :page => @<%= singular_name %>_pages.previous_page %>    <%% end -%>    <%%= @<%= singular_name %>_pages.range %> of <%%= @<%= singular_name %>_pages.count %>    <%% if @<%= singular_name %>_pages.next_page -%>      <%%= link_to 'Next &gt;&gt;', :action => 'list', :page => @<%= singular_name %>_pages.next_page %>    <%% end -%>  </div><%% end %></div>