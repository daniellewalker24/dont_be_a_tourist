<%% @page_title = "" %><h1><%%= h @<%= singular_name %>.name %></h1><%%= short_text_date(@<%= singular_name %>.date) %><%%= render :partial => 'navigation' %><div id="news"><p>  <%% if @<%= singular_name %>.has_picture1? -%>    <%%= thumbnail_tag(@<%= singular_name %>.picture1, :width => 100, :border => "0", :alt => h(@<%= singular_name %>.picture1.image_alt)) %>  <%% end -%>  <%%= @<%= singular_name %>.body %></p>  <%% tags = @<%= singular_name %>.tag_array -%>  <%% unless tags.empty? -%>    <div class="category">(Within categories:<%% for tag in tags -%>       <%%= link_to(tag.humanize, :controller => '<%= feature_name %>', :action => 'list', :tag => tag)%>    <%% end %>)</div>  <%% end -%>    <hr />        <%%= link_to 'Go back', :action => 'list' %> to the <%= print_plural_name %>.</div>