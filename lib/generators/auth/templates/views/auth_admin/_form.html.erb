<%% form_for(<%= singular_name %>, :url => {}, :html => {:method => "post", :multipart => true}) do |f| %>
    
  <p>
    <%%= f.label :name %>
    <%%= f.text_field :name %>
  </p>

  <p>
    <%%= f.label :username %>
    <%%= f.text_field :username %>
  </p>

  <p>
    <%%= f.label :password %>
    <%%= f.password_field :password %>
  </p>
  
  
  <p>
    <%%= f.label :password_confirmation %>
    <%%= f.password_field :password_confirmation %>
  </p>
 
  <p>
    <%%= f.label :address1 %>
    <%%= f.text_field :address1 %>
  </p>
    
  <p>
    <%%= f.label :address2 %>
    <%%= f.text_field :address2 %>
  </p>
    
  <p>
    <%%= f.label :address3 %>
    <%%= f.text_field :address3 %>
  </p>
    
  <p>
    <%%= f.label :postcode %>
    <%%= f.text_field :postcode %>
  </p>

    
  <p>
    <%%= f.label :phone %>
    <%%= f.text_field :phone %>
  </p>

  <p>
    <%%= f.label :email %>
    <%%= f.text_field :email %>
  </p>

  <p>
    <%%= f.label :email_confirmation %>
    <%%= f.text_field :email_confirmation, {:value => <%= singular_name %>.email_confirmation ||= <%= singular_name %>.email} %>
  </p>

  <p>
    <%% if <%= singular_name %>.new_record? %>
    <%%= f.submit "Create", :class => 'submit' %>
    <%% else %>
    <%%= f.submit "Update", :class => 'submit' %>
    <%% end %>
  </p>
<%% end %>
